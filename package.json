  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/traverse": "^7.4.0",
    "@babel/types": "^7.4.0",
    "@types/babel-types": "^7.0.6",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.7",
    "babel-jest": "^24.5.0",
    "import-sort-style-alexandria": "^2.0.0",
    "jest": "^24.5.0",
    "jest-config": "^24.5.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.10.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.3.4000"
  },
  "importSort": {
    ".js, .spec.js": {
