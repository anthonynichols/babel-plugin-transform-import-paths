    "import-sort-style-alexandria": "^2.0.1",
    "jest": "^26.0.1",
    "jest-config": "^26.0.1",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
